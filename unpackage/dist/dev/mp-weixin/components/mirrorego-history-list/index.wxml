<view class="history-list"><view data-event-opts="{{[['tap',[['handlerClickFilter',['$event']]]]]}}" class="filter-box" bindtap="__e"><text>{{taskTypeName}}</text><image src="/static/filter.png" mode></image></view><block wx:if="{{historyList.length}}"><view class="list-box"><block wx:for="{{historyList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item"><view class="date-box"><view class="week"><text class="day">{{item.day}}</text><text class="weekday">{{item.weekday}}</text></view><view class="date">{{item.year+"."+item.month}}</view></view><view data-event-opts="{{[['tap',[['handlerClick',['view','$0'],[[['historyList','',index]]]]]]]}}" class="item-container" bindtap="__e"><view class="special-info"><view class="content-box"><view class="title cut_font">{{item.title}}</view><view class="des cut_font">{{item.des}}</view><view class="time">{{item.time}}</view></view><block wx:if="{{item.image}}"><image src="{{item.image}}" mode></image></block></view><view class="operate-box"><view class="custom-title cut_font_2">{{item.customTitle}}</view><view class="operate"><image src="../../static/delete-icon.png" mode data-event-opts="{{[['tap',[['handlerDelete',['$0'],[[['historyList','',index]]]]]]]}}" catchtap="__e"></image><image src="../../static/share-icon.png" mode data-event-opts="{{[['tap',[['handlerClick',['share','$0'],[[['historyList','',index]]]]]]]}}" catchtap="__e"></image></view></view></view></view></block></view></block><block wx:else><view class="empty-box"><image src="../../static/no_data.png" mode></image><text class="text">暂无书写记录</text></view></block></view>